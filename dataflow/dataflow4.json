{
	"name": "dataflow4",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "source1"
				},
				{
					"name": "TARGETSCD"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "lookup1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> source1",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> TARGETSCD",
				"source1, TARGETSCD lookup(CID == ERROR_FUNCTION(''),",
				"     multiple: false,",
				"     pickup: 'first',",
				"     asc(ERROR_FUNCTION(''), true),",
				"     broadcast: 'auto')~> lookup1"
			]
		}
	}
}